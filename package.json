{
  "name": "beethovenx-token",
  "version": "1.0.0",
  "license": "MIT",
  "types": "./types/index.ts",
  "description": "Core contracts for the BeethovenX token protocol",
  "files": [
    "abi",
    "artifacts",
    "contracts",
    "deployments",
    "exports",
    "types"
  ],
  "author": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beethovenxfi/beethovenx-token"
  },
  "bugs": {
    "url": "https://github.com/beethovenxfi/beethovenx-token/issues"
  },
  "homepage": "https://github.com/beethovenxfi/beethovenx-token#readme",
  "keywords": [
    "beethovenx",
    "ethereum"
  ],
  "scripts": {
    "build": "hardhat compile",
    "console": "hardhat console",
    "testnet:deploy": "yarn ropsten:deploy && yarn rinkeby:deploy && yarn goerli:deploy && yarn kovan:deploy && yarn moonbase:deploy && yarn arbitrum:deploy",
    "mainnet:deploy": "hardhat --network fantom deploy",
    "mainnet:verify": "hardhat --network fantom etherscan-verify --solc-input",
    "mainnet:export": "hardhat --network fantom export --export exports/mainnet.json",
    "rinkeby:deploy": "hardhat --network rinkeby deploy",
    "rinkeby:verify": "hardhat --network rinkeby etherscan-verify --solc-input",
    "rinkeby:export": "hardhat --network rinkeby export --export exports/rinkeby.json",
    "export": "hardhat export --export-all exports/deployments.json",
    "test": "hardhat test --verbose",
    "test:coverage": "cross-env NODE_OPTIONS=\"--max-old-space-size=2048\" hardhat coverage",
    "test:gas": "cross-env REPORT_GAS=true yarn test",
    "prettier": "prettier --write test/**/*.ts && prettier --write contracts/**/*.sol",
    "lint": "yarn prettier && solhint -c .solhint.json contracts/**/*.sol",
    "prepublishOnly": "yarn run build && node scripts/prepublish.js",
    "transferOwnership": "ts-node -r dotenv/config --transpile-only scripts/transferOwnership.ts",
    "approveERC20": "ts-node -r dotenv/config --transpile-only scripts/approveERC20.ts",
    "balanceOf": "ts-node -r dotenv/config --transpile-only scripts/balanceOf.ts",
    "reliquary": "ts-node -r dotenv/config --transpile-only scripts/reliquary.ts",
    "deploy": "ts-node -r dotenv/config --transpile-only scripts/deploy.ts",
    "verify": "ts-node -r dotenv/config --transpile-only scripts/verify.ts"
  },
  "devDependencies": {
    "@boringcrypto/boring-solidity": "boringcrypto/BoringSolidity",
    "@gnosis.pm/safe-core-sdk": "^0.3.1",
    "@gnosis.pm/safe-ethers-adapters": "^0.1.0-alpha.3",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.6",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.5",
    "@nomiclabs/hardhat-solhint": "^2.0.0",
    "@nomiclabs/hardhat-vyper": "^3.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.7.3",
    "@tenderly/hardhat-tenderly": "^1.0.0",
    "@typechain/ethers-v5": "^5.0.0",
    "@types/inquirer": "^8.1.3",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.31",
    "@uniswap/v2-core": "github:uniswap/uniswap-v2-core.git#27f6354bae6685612c182c3bc7577e61bc8717e3",
    "base64-sol": "^1.1.0",
    "chai": "^4.2.0",
    "chalk": "^4.1.2",
    "commander": "^8.2.0",
    "cross-env": "^7.0.0",
    "dotenv": "^10.0.0",
    "ethereum-waffle": "^3.2.0",
    "ethers": "^5.0.0",
    "hardhat": "^2.12.1",
    "hardhat-abi-exporter": "^2.0.8",
    "hardhat-deploy": "^0.9.2",
    "hardhat-deploy-ethers": "^0.3.0-beta.11",
    "hardhat-gas-reporter": "^1.0.0",
    "hardhat-preprocessor": "^0.1.0",
    "hardhat-spdx-license-identifier": "^2.0.0",
    "hardhat-typechain": "^0.3.5",
    "hardhat-watcher": "^2.0.0",
    "inquirer": "^8.1.5",
    "moment": "^2.29.1",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.5",
    "solidity-coverage": "^0.7.13",
    "ts-generator": "^0.1.1",
    "ts-node": "^9.1.1",
    "typechain": "^4.0.2",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "@balancer-labs/v2-interfaces": "^0.4.0",
    "hardhat-ethers": "^1.0.1"
  }
}
